{
  "name": "tooltip-vue-js",
  "version": "1.0.1",
  "repository": "https://github.com/b13oleksandr/tooltip-vue.git",
  "homepage": "https://b13oleksandr.github.io/tooltip-vue",
  "description": "Vuejs tooltip",
  "license": "MIT",
  "keywords": [
    "tooltip",
    "vue-tooltip",
    "tooltip-vue-js",
    "vue js tooltip",
    "vue tooltip"
  ],
  "main": "dist/tooltip-vue.umd.js",
  "module": "dist/tooltip-vue.esm.js",
  "unpkg": "dist/tooltip-vue.min.js",
  "scripts": {
    "lint": "vue-cli-service lint",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/tooltip-vue.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/tooltip-vue.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/tooltip-vue.min.js"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^13.0.2",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^6.2.2",
    "prettier": "^2.2.1",
    "rollup": "^1.17.0",
    "rollup-plugin-vue": "^5.0.1",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11",
    "vuepress": "^1.8.2"
  },
  "files": [
    "dist/*"
  ]
}
